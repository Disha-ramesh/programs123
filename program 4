#include<stdlib.h>
#include<stdio.h>
#define MAX 100
int isprime(int n)
{
    for(int i=2;i<n;i++)
        if(n%i==0)
            return 0;
            
    return 1;
}
int main()
{
    int n,i,j=0;
    printf("enter the number\n");
    scanf("%d",&n);
    i=isprime(n);
    if(i==1)
        printf("prime number\n");
    else
        printf("not a prime number\n");
}
